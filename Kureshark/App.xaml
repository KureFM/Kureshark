<Application x:Class="Kureshark.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <FontFamily x:Key="UbuntuMono">../Font/#Ubuntu Mono</FontFamily>
        <FontFamily x:Key="UbuntuMonoBold">../Font/#Ubuntu Mono Bold</FontFamily>
        <FontFamily x:Key="Consolas">../Font/#Consolas</FontFamily>
        <Brush x:Key="KuresharkGreen">#04a000</Brush>
    </Application.Resources>
</Application>